{
  "name": "@tcool86/zylem",
  "version": "0.2.2",
  "description": "A powerful and easy-to-use framework for creating simple 3D digital interactive applications using TypeScript.",
  "license": "MIT",
  "author": "Tim Cool",
  "contributors": [
    {
      "name": "Tim Cool",
      "email": "tcoolpg@gmail.com",
      "url": "https://github.com/tcool86"
    }
  ],
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": null
    }
  },
  "keywords": [
    "game",
    "three",
    "typescript",
    "rapier3d",
    "physics",
    "framework",
    "vite",
    "simple",
    "interactive"
  ],
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build && typedoc",
    "build:watch": "tsc && vite build --watch && typedoc",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "test": "npx vitest --reporter=verbose",
    "coverage": "vitest run --coverage",
    "start": "vite",
    "dev": "vite --host && typedoc",
    "serve": "vite preview",
    "docs": "yarn typedoc"
  },
  "dependencies": {
    "@dimforge/rapier3d": "^0.11.2",
    "@dimforge/rapier3d-compat": "^0.11.2",
    "@simplyianm/legend-state": "^1.6.2",
    "@types/dat.gui": "^0.7.13",
    "@types/howler": "^2.2.11",
    "bitecs": "^0.3.40",
    "dat.gui": "^0.7.9",
    "function-bind": "^1.1.1",
    "has": "^1.0.3",
    "howler": "^2.2.3",
    "is-core-module": "^2.12.0",
    "lil-gui": "^0.20.0",
    "nanoid": "^3.3.6",
    "nipplejs": "^0.10.1",
    "path-parse": "^1.0.7",
    "picocolors": "^1.0.0",
    "resolve": "^1.22.3",
    "source-map-js": "^1.0.2",
    "stats.js": "^0.17.0",
    "supports-preserve-symlinks-flag": "^1.0.0",
    "three": "0.172.0",
    "three-instanced-uniforms-mesh": "^0.52.0",
    "three-spritetext": "^1.8.1",
    "ts-mixer": "^6.0.4",
    "tsimp": "^2.0.11"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/pluginutils": "^5.1.0",
    "@types/node": "^20.10.1",
    "@types/three": "0.171.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-istanbul": "^2.0.5",
    "@vitest/coverage-v8": "^2.0.5",
    "babel-plugin-inline-import": "^3.0.0",
    "esbuild": "^0.18.20",
    "eslint": "^9.17.0",
    "happy-dom": "^14.12.3",
    "jest": "^29.6.2",
    "prettier": "2.6.2",
    "rollup": "^4.6.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-typescript-paths": "^1.3.0",
    "rollup-plugin-visualizer": "^5.9.3",
    "tslib": "^2.4.0",
    "typedoc": "^0.25.4",
    "typescript": "^5.6.2",
    "vite": "^4.2.1",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-glsl": "^1.3.0",
    "vite-plugin-progress": "^0.0.2",
    "vitest": "^2.0.5"
  },
  "pnpm": {
    "overrides": {
      "postcss@<8.4.31": ">=8.4.31",
      "vite@>=4.0.0 <=4.5.1": ">=4.5.2",
      "vite@>=4.0.0 <=4.5.2": ">=4.5.3",
      "braces@<3.0.3": ">=3.0.3",
      "vite@>=4.2.0 <4.2.3": ">=4.2.3"
    }
  }
}
