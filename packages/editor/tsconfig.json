{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "composite": false,
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src/**/*",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
  ]
}