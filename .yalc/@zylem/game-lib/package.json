{
  "name": "@zylem/game-lib",
  "version": "0.3.16",
  "type": "module",
  "description": "A powerful and easy-to-use framework for creating simple 3D digital interactive applications using TypeScript.",
  "license": "MIT",
  "author": "Tim Cool",
  "repository": {
    "type": "git",
    "url": "https://github.com/zylem-game-lib/zylem.git"
  },
  "contributors": [
    {
      "name": "Tim Cool",
      "email": "tcoolpg@gmail.com",
      "url": "https://github.com/tcool86"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": null
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./camera": {
      "types": "./dist/camera.d.ts",
      "import": "./dist/camera.js"
    },
    "./stage": {
      "types": "./dist/stage.d.ts",
      "import": "./dist/stage.js"
    },
    "./entities": {
      "types": "./dist/entities.d.ts",
      "import": "./dist/entities.js"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "import": "./dist/actions.js"
    }
  },
  "keywords": [
    "game",
    "three",
    "typescript",
    "rapier3d",
    "physics",
    "framework",
    "vite",
    "simple",
    "interactive"
  ],
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=10.0.0",
    "pnpm": ">=10.14.0"
  },
  "scripts": {
    "build": "tsc && vite build && typedoc",
    "build:watch": "tsc && vite build --watch && typedoc",
    "build:preview": "vite build --mode development",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "npx vitest --reporter=verbose",
    "coverage": "vitest run --coverage",
    "start": "vite",
    "dev": "vite --host && typedoc",
    "serve": "npm run build:preview && vite preview --outDir dist-preview",
    "docs": "yarn typedoc"
  },
  "dependencies": {
    "@dimforge/rapier3d": "^0.11.2",
    "@dimforge/rapier3d-compat": "^0.11.2",
    "@kobalte/core": "^0.13.11",
    "@types/dat.gui": "^0.7.13",
    "@types/howler": "^2.2.12",
    "@zylem/assets": "^0.1.0",
    "bitecs": "^0.3.40",
    "dat.gui": "^0.7.9",
    "es-toolkit": "^1.39.10",
    "function-bind": "^1.1.2",
    "has": "^1.0.4",
    "howler": "^2.2.4",
    "is-core-module": "^2.16.1",
    "lil-gui": "^0.20.0",
    "lucide-solid": "^0.511.0",
    "nanoid": "^5.1.5",
    "path-parse": "^1.0.7",
    "picocolors": "^1.1.1",
    "resolve": "^1.22.10",
    "solid-js": "^1.9.9",
    "source-map-js": "^1.2.1",
    "stats.js": "^0.17.0",
    "supports-preserve-symlinks-flag": "^1.0.0",
    "three": "0.180.0",
    "three-addons": "^1.2.0",
    "three-full": "^28.0.2",
    "three-instanced-uniforms-mesh": "^0.52.4",
    "three-spritetext": "^1.10.0",
    "tsimp": "^2.0.12",
    "valtio": "^2.1.7"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "yalcSig": "0158136c1bac8e88fd9e0d7f90cf30de"
}
